{
  "assistant": {
    "default_model": {
      "provider": "google",
      "model": "gemini-2.5-flash-preview-04-17"
    },
    "version": "2"
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 500
    }
  },
  "file_scan_exclusions": [".yarn/", "**/redoc.standalone.js"],
  "inlay_hints": {
    "enabled": true
  },
  "relative_line_numbers": true,
  "vim_mode": true,

  "terminal": {
    "shell": {
      "with_arguments": {
        "program": "/bin/zsh",
        "args": ["--login"]
      }
    }
  },

  // Theme
  "theme": {
    "mode": "system",
    "dark": "Catppuccin Mocha",
    "light": "Catppuccin Macchiato"
  },
  "icon_theme": {
    "mode": "system",
    "dark": "Catppuccin Macchiato",
    "light": "Catppuccin Macchiato"
  }
}
